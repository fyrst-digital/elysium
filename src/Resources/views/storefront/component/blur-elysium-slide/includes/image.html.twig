{% block blur_elysium_slide_image %}
    {% set containerClasses = [
        'blur-elysium-slide-image'
    ] %}

    {% if imageMedia is not defined %}
        {% set imageMedia = slideData.presentationMedia %}
    {% endif %}

    {% block blur_elysium_slide_image_element %}
        {% if imageMedia %}           
            <div class="{{ containerClasses|join(' ') }}">
                {% sw_thumbnails 'elysium-slide-thumbnails' with {
                    media: imageMedia
                } %}
            </div>
        {% endif %}
    {% endblock %}
{% endblock %}